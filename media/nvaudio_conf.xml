      <ctl name="ADMA3 MUX" val="SRC"/>
      <ctl name="SRC MUX" val="AAC-DEC1"/>
      <ctl name="AAC-DEC1 MUX" val="APM-IN2"/>
      <!-- Set default priority for offload APM -->
      <ctl name="APM2 Priority" val="48"/>
    </path>

