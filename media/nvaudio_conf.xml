                start_threshold="511"
                stop_threshold="512"
                silence_threshold="0"
                avail_min="1"/>

    <pcm_config name="music-capture"
                device_id="0"
                rate="48000"
                channels="2"
                bps="16"
                period_size="256"
                period_count="2"
                start_threshold="1"
                stop_threshold="512"
                silence_threshold="0"
