    <pcm_config name="bt-sco-playback"
                device_id="1"
                rate="8000"
                channels="1"
                bps="16"
                period_size="128"
                period_count="4"
                start_threshold="511"
                stop_threshold="512"
                silence_threshold="0"
                avail_min="1"/>

    <pcm_config name="bt-sco-capture"
